<?xml version="1.0" encoding="UTF-8"?>

<ivysettings>
  <settings defaultResolver="chained" />

  <resolvers>
    <chain name="chained" returnFirst="true">
    <ibiblio name="ibiblio" m2compatible="true" />
    </chain>
  </resolvers>

  <caches default="storage">
    <cache name="storage" basedir="${ivy.cache.dir}"/>
  </caches>
</ivysettings>
